{
  "name": "@hammerstone/refine-react",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./flavors/default": {
      "require": "./dist/flavors/default.cjs",
      "import": "./dist/flavors/default.mjs"
    },
    "./flavors/tailwind": {
      "require": "./dist/flavors/tailwind.cjs",
      "import": "./dist/flavors/tailwind.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "flavors/default": [
        "dist/flavors/default.d.ts"
      ],
      "flavors/tailwind": [
        "dist/flavors/tailwind.d.ts"
      ]
    }
  },
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest --watch"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.0",
    "jest": "^27.4.7",
    "refine-core": "*",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "deepmerge": "^4.2.2"
  }
}
